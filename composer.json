{
  "name": "kambo/deinj",
  "description": "Dependency injection container",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "psr/container" : "1.0"
  },
  "require-dev": {
    "pds/skeleton": "~1.0",
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Kambo\\Deinj\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kambo\\Tests\\Deinj\\": "tests/"
    }
  },    
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs"
    ],
    "phpunit": "php vendor/bin/phpunit",
    "phpcs": "php vendor/bin/phpcs"
  }
}
